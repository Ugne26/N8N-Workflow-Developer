      "parameters": {
        "jsCode": "const timeZone = \"Europe/Bucharest\";\nconst today = new Date().toLocaleDateString(\"en-CA\", { timeZone });\n\nreturn items\n  .map(item => {\n    const pubDate = new Date(item.json.pubDate || item.json.isoDate || item.json.published);\n    const postDate = pubDate.toLocaleDateString(\"en-CA\", { timeZone });\n\n    return {\n      json: {\n        date: postDate,\n        guid: item.json.guid || item.json.link || item.json.id || item.json.title,\n        title: item.json.title || \"(no title)\",\n        link: item.json.link || \"\",\n        creator: item.json.author || $('Convert to RSS').first().json.creator || \"Unknown\",\n        platform: \"YouTube\"\n      }\n    };\n  })\n  .filter(item => item.json.date === today);\n"
      },
