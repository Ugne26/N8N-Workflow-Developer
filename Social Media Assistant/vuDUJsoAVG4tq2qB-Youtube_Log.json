    {
      "parameters": {
        "jsCode": "const link = $input.first().json.input_link || '';\nlet feedUrl = '';\nlet creator = 'Unknown';\n\nif (link.includes('youtube.com/channel/')) {\n  const channelId = link.split('/channel/')[1].split(/[/?#]/)[0];\n  feedUrl = `https://www.youtube.com/feeds/videos.xml?channel_id=${channelId}`;\n  creator = channelId;\n} else if (link.includes('youtube.com/@')) {\n  const username = link.split('/@')[1].split(/[/?#]/)[0];\n  feedUrl = `https://www.youtube.com/feeds/videos.xml?user=${username}`;\n  creator = username;\n} else {\n  throw new Error('Invalid YouTube link');\n}\n\nreturn [{\n  json: {\n    feed_url: feedUrl,\n    creator: creator\n  }\n}];\n"
      },
